    "claim": "<p style=\"text-align:right\"><span dir=\"RTL\" lang=\"AR-SA\">فرض حظر تجوال شامل&nbsp;في مصر.</span></p>\r\n",
    "country": "EG",
    "created_at": "2020-12-30 12:46:43",
    "created_by": "5f39b5a51dd585076701cb94",
    "deleted_at": null,
    "feature": 0,
    "feedback_neg": 5,
