31 ,  4.016]], dtype=float16)"]},"metadata":{"tags":[]},"execution_count":24}]},{"cell_type":"markdown","metadata":{"id":"zuEiTrBa6C_B"},"source":["# Predict"]},{"cell_type":"code","metadata":{"id":"PQ6ieb6W6B29","executionInfo":{"status":"ok","timestamp":1628754705143,"user_tz":-180,"elapsed":36,"user":{"displayName":"braa eyad","photoUrl":"","userId":"12525524382478593078"}}},"source":["class Dataset():\n","    def __init__(self, text=None, target= None, model_name = None, max_len= None, label_map= None):\n","      super(BERTDataset).__init__()\n","      self.text = text\n","      self.target = target\n","      self.tokenizer_name = model_name\n","      self.tokenizer = AutoTokenizer.from_pretrained(model_name)\n","      self.max_len = max_len\n","      self.label_map = label_map\n","      \n","\n","    def __len__(self):\n","      return len(self.text)\n","\n","    def __getitem__(self,item):\n","      text = str(self.text[item])\n","      text = \" \".join(text.split())\n","\n","\n","        \n","      input_ids = self.tokenizer.encode(\n","          text,\n","          add_special_tokens=True,\n","          max_length=self.max_len,\n","          truncation='longest_first'\n","      )     \n","    \n","      attention_mask = [1] * len(input_ids)\n","\n","      # Zero-pad up to the sequence length.\n","      padding_length = self.max_len - len(input_ids)\n","      input_ids = input_ids + ([self.tokenizer.pad_token_id] * padding_length)\n","      attention_mask = attention_mask + ([0] * padding_length)    \n","      \n","      return InputFeatures(input_ids=input_ids, attention_mask=attention_mask)"],"execution_count":25,"outputs":[]},{"cell_type":"code","metadata":{"id":"FF_sYnWz6JUt","colab":{"base_uri":"https://localhost:8080/","height":667},"executionInfo":{"status":"ok","timestamp":1628754770180,"user_tz":-180,"elapsed":65063,"user":{"displayName":"braa eyad","photoUrl":"","userId":"12525524382478593078"}},"outputId":"94867800-aca3-46e6-e160-eb8f6f74ecfe"},"s